	<script type="text/javascript">
	
		$(document).ready(function() {
			
			// Handle the focus and blur of the JSON url input
			$('#inputUrl').focus(encoder.handleInputUrlFocus);
			$('#inputUrl').blur(encoder.handleInputUrlBlur);			
			
			$('#inputString').keydown(encoder.handleIputStringKeydown);
			$('#inputUrl').keydown(encoder.handleInputUrlKeydown);
			
			encoder.initInputUrl();
			
			$('#encodeButton').click(encoder.encode);
			$('#decodeButton').click(encoder.decode);
			
			$('#yesButton').click(encoder.decodeBinary);
			$('#noButton').click(encoder.decodeString);			
			
			$('#dialog-confirm').dialog({
				resizable: false,
				height: 160,
				width: 350,
				modal: true,
				autoOpen: false,
				closeOnEscape: false
			});
			
		});

		var encoder = {

			// The default URL to display in the JSON URL control
			defaultUrl: "http://www.example.com/myfile",

			/**
			* Decodes the content as a binary file
			*/
			decodeBinary: function() {
				$('#dialog-confirm').dialog('close');
				$('#isBinary').val('true');
				$('#action').val('decode');
				$('#form').trigger('submit');				
			},
			
			/**
			* Decodes the content as a string
			*/
			decodeString: function() {
				$('#dialog-confirm').dialog("close");
				$('#isBinary').val('false');
				$('#action').val('decode');
				$('#form').trigger('submit');				
			},
			
			/**
			* If someone is changing the content of the textarea, clear the url part
			*/
			handleIputStringKeydown: function() {
				$('#inputUrl').val('');
				encoder.initInputUrl();
			},
			
			/**
			* If someone changes the url, empty the text area
			*/
			handleInputUrlKeydown: function() {
				$('#inputString').val('');
			},
			
			/**
			* Initialize the json url control
			*/
			initInputUrl: function() {
							
				if ($.string($('#inputUrl').val()).blank()
						|| $('#inputUrl').val() == encoder.defaultUrl) {
					$('#inputUrl').css('color', '#CCCCCC').val(encoder.defaultUrl);
				} else {
					$('#inputUrl').css('color', '#404040');
				}			
				
			},
			
			/**
			* Defaults the JSON url control to display a bogus example of a URL
			*/
			defaultInputUrl: function() {
				$('#inputUrl').css('color', '#CCCCCC').val(encoder.defaultUrl);
			},

			/**
			* Handles the focus even of the JSON url control.  Removes the default string if
			* present and resets the color to black.
			*/
			handleInputUrlFocus: function() {
				var value = $('#inputUrl').val();
				if (value === encoder.defaultUrl) {
					$('#inputUrl').css('color', '#404040').val('');
				}
			},

			/**
			* Handles the blur event of the JSON url control.  Sets the default string back in if the
			* field is empty and sets back the color to a light gray
			*/
			handleInputUrlBlur: function() {
				var value = $('#inputUrl').val();
				if ($.string(value).blank()) {
					encoder.defaultInputUrl();
				}
			},

			/**
			* Sets the target attribute of the form
			*/
			setForceNewWindow: function() {
				
				var val = $('#forceNewWindow').val();
				
				if (val === 'true') {
					$('#form').attr('target', '_blank');
				} else {
					$('#form').attr('target', '_self');
				}
				
			},			
			
			/**
			* Submits the form to escape a given string
			*/
			encode: function() {
				encoder.setForceNewWindow();
				$('#action').val('encode');
				$('#form').trigger('submit');
			},
			
			/**
			* Submits the form to unescape a given string
			*/
			decode: function() {
				encoder.setForceNewWindow();
				$('#dialog-confirm').dialog("open");
			}
			
		};

	</script>	  		
